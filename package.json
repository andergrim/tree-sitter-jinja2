{
  "name": "tree-sitter-jinja2",
  "version": "1.0.0",
  "description": "Tree-sitter grammar for Jinja2 templates",
  "main": "bindings/node",
  "scripts": {
    "test": "tree-sitter generate && tree-sitter parse example-file.jinja2"
  },
  "author": "Stephan Seitz",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git://github.com/andergrim/tree-sitter-jinja2.git"
  },
  "tree-sitter": [
    {
      "scope": "source.jinja2",
      "file-types": [
        "jinja2",
        "jinja"
      ]
    }
  ],
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.7"
  }
}
